generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Jobs {
  job_id      String   @id @default(cuid())
  company     String?
  link        String?
  createdOn   DateTime @default(now())
  shift String?
  location       String?
  title       String?
  index  Int

  @@map("jobs")
}
